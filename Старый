    private void ListBox1_Click(object sender, EventArgs e)
    {
      string atr = ListBox1.SelectedIndex.ToString();
            
      if (Convert.ToInt32(atr) == 2 &amp;&amp; RadioButton1.Checked ==true)
      {
        Label6.Visible = true;
        ComboBox2.Visible = true;
      }
      if (Convert.ToInt32(atr) != 2 || RadioButton2.Checked ==true)
      {
      Label6.Visible = false;
      ComboBox2.Visible = false;
    }
      Label7.Text = ListBox1.SelectedIndex.ToString();
      }

    private void Form1_Load(object sender, EventArgs e)
    {
      string atr = ListBox1.SelectedIndex.ToString();
            
      if (RadioButton1.Checked == false &amp;&amp; RadioButton2.Checked == false)
      {
        ListBox1.FillData();
      }
      if (ComboBox1.Text ==&quot;Трехсторонний договор2&quot;)
      {
        Label8.Visible = true;
        Label9.Visible = true;
        ComboBox3.Visible = true;
        TextBox2.Visible = true;
      }
    } 


 private void btnOk_Click(object sender, EventArgs e)
    {
      if (RadioButton1.Checked == true)
      {
         Page3.Visible =  false;
      }
      if (RadioButton2.Checked == true)
      {
        Page1.Visible =  false;
      }
     
    //--------------------------------
      string atr2 = ListBox1.SelectedIndex.ToString();
      
      RichTextBox1.Text = Report.GetColumnValue(&quot;Table1.Комментарий&quot;).ToString();
      TextBox1.Text= Report.GetColumnValue(&quot;Table1.Размер_скидки&quot;).ToString(); 
            
      if (ComboBox1.Text ==&quot;Двухсторонний договор&quot; &amp;&amp; RadioButton1.Checked == true)
      {
        Data3.Visible = false ;
        DataFooter1.Visible = false;
        Data5.Visible = false;
        Data41.Visible = false ;
        DataFooter6.Visible = false;

        //Page3.Visible =  false;
        //string atr1 = ListBox1.SelectedIndex.ToString();
        if (Convert.ToInt32(atr2) == 1)
        {
          Data16.Visible = true;
        }
        if (Convert.ToInt32(atr2) == 6)
        {
          Data17.Visible = true;
        }
        if (Convert.ToInt32(atr2) == 2)
        {
          Data18.Visible = true;
        }
        if (Convert.ToInt32(atr2) == 3)
        {
          Data19.Visible = true;
        }
        if (Convert.ToInt32(atr2) == 4)
        {
          Data20.Visible = true;
        }
        if (Convert.ToInt32(atr2) == 5)
        {
          Data21.Visible = true;
        }
        if (Convert.ToInt32(atr2) == 0)
        {
          Data58.Visible = true;
        } 
      }  
      
      if  (ComboBox1.Text ==&quot;Трехсторонний договор&quot; &amp;&amp; RadioButton1.Checked == true)  
      
      {
        Data4.Visible = false;
        DataFooter2.Visible = false ;
        Data6.Visible = false;
        Data41.Visible = false ;
        DataFooter6.Visible = false;

        if (Convert.ToInt32(atr2) == 1)                      
        {
          Data10.Visible = true;
        }
        if (Convert.ToInt32(atr2) == 6)
        {
          Data11.Visible = true;
        }
        if (Convert.ToInt32(atr2) == 2)
        {
          Data12.Visible = true;
        }
        if (Convert.ToInt32(atr2) == 3)
        {
          Data13.Visible = true;
        }
        if (Convert.ToInt32(atr2) == 4)
        {
          Data14.Visible = true;
        }
        if (Convert.ToInt32(atr2) == 5)
        {
          Data15.Visible = true;
        } 
        if (Convert.ToInt32(atr2) == 0)
        {
          Data57.Visible = true;
        } 
      }
      if  (ComboBox1.Text ==&quot;Трехсторонний договор2&quot; &amp;&amp; RadioButton1.Checked == true)  
      
      {
        Data4.Visible = false;
        DataFooter2.Visible = false ;
        Data6.Visible = false;
        Data3.Visible = false ;
        DataFooter1.Visible = false;

        if (Convert.ToInt32(atr2) == 1)                      
        {
          Data42.Visible = true;
        }
        if (Convert.ToInt32(atr2) == 6)
        {
          Data43.Visible = true;
        }
