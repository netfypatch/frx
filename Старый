 рублей')&#13;&#10;else REPLACE(dbo.СуммаПрописью(CAST(replace ((цена1к - цена1к * @Скидка /100), ',', '.') AS money)), '', ' рублей') end) as НоваяСуммаЗаГодПрописью,&#13;&#10;CAST(replace ((ПолнаяЦенаВыбор - ПолнаяЦенаВыбор * @Скидка /100), ',', '.') AS money) AS НоваяСуммаЗаВесьПериод,&#13;&#10;REPLACE(dbo.СуммаПрописью(CAST(replace ((ПолнаяЦенаВыбор - ПолнаяЦенаВыбор * @Скидка /100), ',', '.') AS money)), '', ' рублей') as НоваяСуммаЗаВесьПериодПрописью,&#13;&#10;(case when ТипПлательщика =1 then 'Двухсторонний договор' &#13;&#10;when ТипПлательщика =2 then'Трехсторонний договор'&#13;&#10;when ТипПлательщика =3 then'Трехсторонний договор2' end) as Договор&#13;&#10;from (&#13;&#10;select лд.фио as ФИОстудента, сп.срок_обучения as срок_обучения, з.дистобразование, сп.до, (case when з.дистобразование = 1 and сп.до = 1 then сп.дотполнаяцена else сп.Полнаяцена end) as ПолнаяЦенаВыбор, departmentaddress, kpp, mINN, mRecipient, meks, mbank, mbic, mks, moktmo, mcbc, msokrname,&#13;&#10;&#13;&#10; зк.заказчик as ЗакЮр, зк.сокращение, зк.адрес as Адр, зк.банк, зк.инн, зк.бик, зк.рс, зк.кс, зк.окпо,&#13;&#10;зк.оквд, зк.паспортныеданные as ПД, зк.паспортвыдан as ПДВ, зк.паспортдата as ПаспДата, &#13;&#10;с.фио as ЗаказчикФИО,&#13;&#10;&#13;&#10; convert(varchar,з.датазаключения, 104) as ДатаЗаключения, з.номер_договора,&#13;&#10;лд.СуммаПрописью as затратыПрописью, лд.затраты, сп.Полнаяцена, лд.СуммаПрописью1, &#13;&#10;&#13;&#10;с.фамилия as ФамЗаказ, с.имя as ИмяЗаказ, с.отчество as ОтчЗаказ, с.адрес as адресЗак, с.паспорттип, с.паспортномер, с.паспорткемвыдан, convert(varchar, с.паспортдата, 104) as паспортзакдата,&#13;&#10;с.телефон, с.Email, convert(varchar, с.дата_рождения, 104) as ДРЗакС,&#13;&#10;&#13;&#10;з.допсоглашение, convert(varchar, з.датасоглашения, 104) as датасоглашения, &#13;&#10;convert(money, ROUND(лд.суммазагод,2)) as суммазагод, лд.суммапрописьюзагод, з.ТипПлательщика as ТипПлательщика,&#13;&#10;&#13;&#10;о.Заказчик as ЗакО, о.адрес, о.паспортныеданн